{
    "name": "porter2/porter2",
    "description": "Stem words using the English Porter 2 algorithm",
    "type": "library",
    "license": "GPLv2",
    "autoload": {
        "psr-4": { "markfullmer\\porterstemmer\\": ["src/", "test/"] }
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "suggest": {
        "ext-mbstring": "For best performance",
        "symfony/polyfill-mbstring": "If you can't install ext-mbstring"
    }
}
